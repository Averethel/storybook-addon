{
  "name": "@react-theming/storybook-addon",
  "version": "0.1.0",
  "private": false,
  "main": "dist/index.js",
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "start-storybook": "start-storybook -p 9001 -s public --ci",
    "prepare": "rm -rf dist && babel src --out-dir dist --verbose",
    "dev": "yarn prepare && yarn start-storybook",
    "start": "nodemon --exec yarn dev",
    "build-storybook": "build-storybook -s public",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-devkit": "^1.0.0",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "babel-loader": "^8.0.2",
    "nodemon": "^1.18.9"
  }
}
